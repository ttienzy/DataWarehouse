    public override void CreateNewOutputRows()
    {
        // *** B?N C� TH? THAY ??I NG�Y B?T ??U V� K?T TH�C ? ?�Y ***
        // ----- CẤU HÌNH KHOẢNG THỜI GIAN BẠN MUỐN TẠO -----
        // Hãy đảm bảo khoảng thời gian này bao trùm toàn bộ dữ liệu booking của bạn.
        DateTime startDate = new DateTime(2020, 1, 1);
        DateTime endDate = new DateTime(2030, 12, 31);
        // ----------------------------------------------------

        // Bắt đầu vòng lặp từ ngày bắt đầu đến ngày kết thúc
        for (DateTime currentDate = startDate; currentDate <= endDate; currentDate = currentDate.AddDays(1))
        {
            // Thêm một dòng mới vào bộ đệm đầu ra (output buffer)
            // Tên của buffer chính là tên Output bạn đã đặt ở Bước 1 (ví dụ: DateStreamBuffer)
            DateStreamBuffer.AddRow();

            // Gán giá trị cho từng cột bạn đã định nghĩa
            DateStreamBuffer.DateKey = int.Parse(currentDate.ToString("yyyyMMdd"));
            DateStreamBuffer.FullDate = currentDate;
            DateStreamBuffer.Year = currentDate.Year;
            DateStreamBuffer.Quarter = (currentDate.Month - 1) / 3 + 1;
            DateStreamBuffer.Month = currentDate.Month;

            // Sử dụng văn hóa "vi-VN" để lấy tên Tiếng Việt
            System.Globalization.CultureInfo ci = new System.Globalization.CultureInfo("vi-VN");
            DateStreamBuffer.MonthName = "Tháng " + currentDate.Month.ToString();
            DateStreamBuffer.DayName = ci.DateTimeFormat.GetDayName(currentDate.DayOfWeek);

            // DayOfWeek trong .NET bắt đầu từ 0 (Sunday). SQL thường bắt đầu từ 1.
            DateStreamBuffer.DayOfWeek = (int)currentDate.DayOfWeek + 1;

            // Kiểm tra xem có phải cuối tuần không
            if (currentDate.DayOfWeek == DayOfWeek.Saturday || currentDate.DayOfWeek == DayOfWeek.Sunday)
            {
                DateStreamBuffer.IsWeekend = true;
            }
            else
            {
                DateStreamBuffer.IsWeekend = false;
            }
        }


    }

}
