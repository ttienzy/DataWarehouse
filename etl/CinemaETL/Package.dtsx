                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
    -- Các ID nguồn để Lookup
    b.CustomerId,
    st.MovieId,
    sc.CinemaId,
    b.ShowtimeId,

    -- Ngày để Lookup DateKey
    CAST(b.BookingTime AS DATE) AS BookingDate,

    -- Các số đo (Measures)
    b.TotalAmount,
    b.DiscountAmount,
    b.FinalAmount,
    
    -- === PHẦN TÍNH TOÁN MỚI ===
    -- Đếm số lượng vé từ bảng BookingTickets cho mỗi Booking
    COUNT(bt.Id) AS TotalTickets

FROM 
    dbo.Bookings b
JOIN 
    dbo.Showtimes st ON b.ShowtimeId = st.Id
JOIN 
    dbo.Screens sc ON st.ScreenId = sc.Id
JOIN 
    dbo.BookingTickets bt ON b.Id = bt.BookingId  -- Thêm JOIN quan trọng này

GROUP BY
    b.CustomerId,
    st.MovieId,
    sc.CinemaId,
    b.ShowtimeId,
    CAST(b.BookingTime AS DATE),
    b.TotalAmount,
    b.DiscountAmount,
    b.FinalAmount</property>
